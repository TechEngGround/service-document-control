{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "google",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".ts"
        ]
      }
    }
  },
  "rules": {
    "@typescript-eslint/camelcase": "off",
    "camelcase":"off",
    "new-cap": "off",
    "require-jsdoc": "off",
    "indent": [
      "error",
      2
    ],
    "@typescript-eslint/explicit-function-return-type": "off",
    "@typescript-eslint/interface-name-prefix": "off",
    "@typescript-eslint/member-delimiter-style": [
      "error",
      {
        "multiline": {
          "delimiter": "none",
          "requireLast": false
        },
        "singleline": {
          "delimiter": "semi",
          "requireLast": false
        }
      }
    ],
    "@typescript-eslint/no-explicit-any": "off",
    "consistent-return": "off",
    "func-names": "off",
    "import/extensions": [
      "off",
      {
        "js": "never",
        "ts": "never"
      }
    ],
    "import/prefer-default-export": "off",
    "no-console": [
      "warn",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-nested-ternary": [
      "off"
    ],
    "no-param-reassign": [
      "warn",
      {
        "props": false
      }
    ],
    "no-underscore-dangle": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "lf",
        "printWidth": 100,
        "semi": false,
        "singleQuote": false,
        "tabWidth": 2,
        "trailingComma": "all"
      }
    ]
  }
}